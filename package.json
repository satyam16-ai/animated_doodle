{
  "name": "antigravity",
  "version": "1.0.0",
  "description": "Real-time 2D doodle avatar creator",
  "main": "dist-electron/index.js",
  "scripts": {
    "dev": "npm run build:preload && vite",
    "build:preload": "esbuild src/preload/index.ts --bundle --platform=node --outfile=dist-electron/preload.js --external:electron",
    "build": "npm run build:preload && vite build && electron-builder",
    "preview": "vite preview"
  },
  "keywords": [
    "electron",
    "mediapipe",
    "avatar",
    "doodle"
  ],
  "author": "Antigravity",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/three": "^0.182.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "esbuild": "^0.19.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.28.0"
  },
  "dependencies": {
    "@mediapipe/camera_utils": "^0.3.1632432244",
    "@mediapipe/drawing_utils": "^0.3.1620248257",
    "@mediapipe/face_mesh": "^0.4.1633559619",
    "three": "^0.182.0"
  }
}
